{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=SaanaPayLeaveDB;Trusted_Connection=True; multipleactiveresultsets=True; TrustServerCertificate=Yes"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl":  "http://localhost:5341"}
      }
    ]
  },
  "AllowedHosts": "*"
}
